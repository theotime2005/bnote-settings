{
  "name": "bnote-settings",
  "version": "4.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^22.14.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
